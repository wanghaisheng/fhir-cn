                {
                  "identity": "rim",
                  "map": "N/A - this is sometimes implied by context"
                },
                {
                  "identity": "servd",
                  "map": "./StartDate and ./EndDate"
                }
              ]
            }
          },
          {
            "path": "Identifier.assigner",
            "definition": {
              "short": "Organization that issued id (may be just text)",
