  "structure": [
    {
      "type": "SampledData",
      "base": "http://hl7.org/fhir/Profile/ResourceElement",
      "publish": true,
      "purpose": "This is the defintion for the SampledData datatype",
      "snapshot": {
        "element": [
          {
            "path": "SampledData",
            "definition": {
              "short": "A series of measurements taken by a device",
              "formal": "A series of measurements taken by a device, with upper and lower limits. There may be more than one dimension in the data.",
              "comments": "The data is not interpretable without at least origin, period, and dimensions, but these are optional to allow a separation between the template of measurement and the actual measurement, such as between DeviceCapabilities and DeviceLog.",
              "min": 1,
              "max": "1",
              "type": [
                {
                  "code": "Type"
                }
              ],
              "isModifier": false,
              "mapping": [
                {
                  "identity": "v2",
                  "map": "N/A (but see section 7.14 in v2.7)"
                },
                {
                  "identity": "rim",
                  "map": "SLIST + GLIST"
                }
              ]
            }
          },
          {
            "path": "SampledData.extension",
            "definition": {
              "short": "Additional Content defined by implementations",
              "formal": "May be used to represent additional information that is not part of the basic definition of the resource. In order to make the use of extensions safe and manageable, there is a strict governance applied to the definition and use of extensions. Though any implementer is allowed to define an extension, there is a set of requirements that SHALL be met as part of the definition of the extension.",
              "comments": "there can be no stigma associated with the use of extensions by any application, project, or standard - regardless of the institution or jurisdiction that uses or defines the extensions.  The use of extensions is what allows the FHIR specification to retain a core simplicity for everyone.",
              "synonym": [
                "extensions",
                "user content"
              ],
              "min": 0,
              "max": "*",
              "type": [
                {
                  "code": "Extension"
                }
              ]
            }
          },
          {
            "path": "SampledData.origin",
            "definition": {
              "short": "Zero value and units",
              "formal": "The base quantity that a measured value of zero represents. In addition, this provides the units of the entire measurement series.",
              "min": 1,
              "max": "1",
              "type": [
                {
                  "code": "Quantity"
                }
              ],
              "isModifier": false,
              "mapping": [
                {
                  "identity": "rim",
                  "map": "SLIST.origin"
                }
              ]
            }
          },
          {
            "path": "SampledData.period",
            "definition": {
              "short": "Number of milliseconds between samples",
              "formal": "The length of time between sampling times, measured in milliseconds.",
              "comments": "This is usually a whole number.",
              "min": 1,
              "max": "1",
              "type": [
                {
                  "code": "decimal"
                }
              ],
              "isModifier": false,
              "mapping": [
                {
                  "identity": "rim",
                  "map": "GLIST.increment"
                }
              ]
            }
          },
          {
            "path": "SampledData.factor",
            "definition": {
              "short": "Multiply data by this before adding to origin",
              "formal": "A correction factor that is applied to the sampled data points before they are added to the origin.",
              "min": 0,
              "max": "1",
              "type": [
                {
                  "code": "decimal"
                }
              ],
              "isModifier": false,
              "mapping": [
                {
