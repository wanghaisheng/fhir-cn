        "element": [
          {
            "path": "Other",
            "definition": {
              "short": "Resource for non-supported content",
              "formal": "Other is a conformant for handling resource concepts not yet defined for FHIR or outside HL7's scope of interest.",
              "min": 1,
              "max": "1",
              "type": [
                {
                  "code": "Resource"
                }
              ],
              "isModifier": false,
              "mapping": [
                {
